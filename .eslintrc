{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "quotes": [ 1, "single" ],
    "no-undef": false,
    "global-strict": true,
    "no-extra-semi": 1,
    "no-underscore-dangle": false,
    "no-console": false,
    "no-trailing-spaces": false
  }
}
